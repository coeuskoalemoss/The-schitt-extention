{
"manifest_version": 2,
    "name": "The schitt extention",
    "description": "The schitt extention changes the word 'baby' into 'bébé' on all your webpages.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}